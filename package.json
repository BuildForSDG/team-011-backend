{
  "name": "farmlord",
  "version": "0.0.1",
  "description": "An application that connect Farmer to Landowners",
  "keywords": [
    "node",
    "jest"
  ],
  "private": false,
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prebuild": "rimraf dist .cache coverage",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:cover": "jest --coverage --detectOpenHandles",
    "lint": "eslint \"src/**/*.js\"",
    "build": "parcel build src/server.js --target=node --no-source-maps",
    "watch": "parcel watch src/server.js --target=node",
    "start:dev": "nodemon src/server.js",
    "start": "node dist/server.js"
  },
  "author": {
    "name": "Build For SDG",
    "email": "buildforsdg@andela.com"
  },
  "homepage": "https://github.com/BuildForSDG/js-starter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildForSDG/js-starter.git"
  },
  "bugs": {
    "url": "https://github.com/BuildForSDG/js-starter/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^7.1.0",
    "accesscontrol": "^2.2.1",
    "bcrypt": "^4.0.1",
    "celebrate": "^12.1.1",
    "cloudinary": "^1.21.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.1",
    "http-errors": "^1.7.3",
    "http-status-codes": "^1.4.0",
    "jade": "^1.11.0",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mailgun-js": "0.5",
    "mongoose": "^5.9.11",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "parcel-bundler": "^1.12.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.5.4",
    "mongodb-memory-server": "^6.5.2",
    "mongodb-memory-server-global": "^6.5.2",
    "nodemon": "^2.0.3",
    "prettier-eslint": "^11.0.0",
    "supertest": "^4.0.2"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
